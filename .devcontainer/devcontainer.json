{
  "name": "enlighten-dev",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "docker-compose.dev.yml"
  ],
  "service": "dev",
  "runServices": ["mysql", "redis"],
  "workspaceFolder": "/workspace",
  "overrideCommand": false,
  "remoteUser": "root",
  "postCreateCommand": "mvn -q -B -DskipTests -U dependency:go-offline",
  "forwardPorts": [8080, 5005, 3307, 6379]
}


